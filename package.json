{
  "name": "magic-code",
  "displayName": "Magic Code",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "Magic Code",
      "properties": {
        "magic-code.openai_api_key": {
          "type": "string",
          "default": "sk-<your key here>",
          "description": "OpenAI API KEY"
        },
        "magic-code.templates": {
          "type": "object",
          "default": {},
          "description": "Templates for code processing"
        }
      }
    },
    "commands": [
      {
        "command": "magic-code.processSelectedText",
        "title": "Magic Code: Go!"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "test-quick": "node ./test/runQuickTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "openai": "^3.2.1"
  }
}
